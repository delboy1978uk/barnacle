{
  "name": "delboy1978uk/barnacle",
  "type": "library",
  "description": "Baracle be a simple Dependency Injection Container fer PHP and part o' th' Bone Framework",
  "keywords": [
    "dependency injection",
    "container",
    "PSR-11"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Derek Stephen McLean",
      "email": "delboy1978uk@gmail.com"
    }
  ],
  "require": {
    "php": ">=5.3.0",
    "psr/container": "^1.0",
    "pimple/pimple": "^3.2"
  },
  "require-dev": {
    "codeception/codeception": "^4.1",
    "roave/security-advisories": "dev-master",
    "codeception/module-asserts": "^2.0"
  },
  "autoload": {
    "psr-4": {
      "Barnacle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Barnacle\\Tests\\": "tests/unit/"
    }
  },
  "provide": {
    "psr/container-implementation": "^1.0"
  }
}
