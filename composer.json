{
  "name": "delboy1978uk/barnacle",
  "type": "library",
  "description": "Baracle be a simple Dependency Injection Container fer PHP and part o' th' Bone Framework",
  "keywords": [
    "dependency injection",
    "container",
    "PSR-11"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Derek Stephen McLean",
      "email": "delboy1978uk@gmail.com"
    }
  ],
  "require": {
    "php": ">8.2",
    "pimple/pimple": "^3.2",
    "boneframework/contracts": "^1.0"
  },
  "require-dev": {
    "roave/security-advisories": "dev-latest",
    "delboy1978uk/dev-tools": "v1.0.5"
  },
  "autoload": {
    "psr-4": {
      "Barnacle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Barnacle\\Tests\\": "tests/unit/"
    }
  },
  "provide": {
    "psr/container-implementation": "^1.0"
  }
}
